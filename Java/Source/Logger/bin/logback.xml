<?xml version="1.0" encoding="UTF-8"?>
<configuration debug="false">
  <appender name="FILE" class="ch.qos.logback.core.FileAppender">
    <file>input.log</file>
    <encoder>
      <pattern>%d{dd/MM/yy HH:mm} %-5level %logger{36} - %msg%n</pattern>
    </encoder>
  </appender>

  <root level="debug">
    <appender-ref ref="FILE" />
  </root>
</configuration>